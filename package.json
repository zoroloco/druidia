    "async": "^1.5.0",
    "winston": "^2.1.1",
    "mysql": "^2.10.2",
    "express": "^4.13.4",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "jade": "^1.11.0",
    "body-parser": "^1.15.0",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.0",
    "cookie-parser": "^1.4.3",
    "express-session": "^1.13.0",
    "connect-mongo": "^1.2.1",
    "mobile-detect": "^1.3.5",
    "vhost": "^3.0.2",
    "bower": "^1.7.7",
    "browserify": "^13.1.1",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-typescript": "^3.1.3",
    "gulp-bump": "^2.5.1",
    "gulp-git": "^1.12.0",
