{
  "name": "druidia.net",
  "version": "0.0.30",
  "description": "druidia.net MEAN project.",
  "main": "node index.js",
  "scripts": {
    "start": "node app-server/index.js",
    "clean": "rm -rf app-web/dist && mkdir app-web/dist && touch app-web/dist/bundle.js && mkdir app-web/dist/www",
    "copy_www": "cp -R app-web/www/* app-web/dist/www/",
    "build": "tsc -p app-web",
    "clean_build": "npm run clean && npm run copy_www && npm run build",
    "bundle": "browserify -s main app-web/dist/main.js > app-web/dist/bundle.js",
    "build_bundle": "npm run clean_build && npm run bundle",
    "minify": "uglifyjs app-web/dist/bundle.js --screw-ie8 --compress --mangle --output app-web/dist/bundle.min.js",
    "build_bundle_minify": "npm run clean_build && browserify -s main app-web/dist/main.js > app-web/dist/bundle.js && npm run minify",
    "brun": "npm run build_bundle_minify && npm start",
    "publish": "gulp release",
    "test": "node_modules/karma/bin/karma start test/karma.conf.js"
  },
  "keywords": [
    "druidia.net"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoroloco/druidia.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoroloco/druidia.git/issues",
    "email": "kcenturion@yahoo.com"
  },
  "author": {
    "name": "Kenneth Centurion"
  },
