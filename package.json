    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "minify": "^2.0.13",
    "uglify": "^0.1.5",
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
